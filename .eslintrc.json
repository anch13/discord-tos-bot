{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "root": true,
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-prototype-builtins": "off"
    }
}
